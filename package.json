{
  "name": "nextjs-devtools",
  "version": "0.0.12",
  "private": false,
  "description": "Next.js devtools CLI",
  "keywords": [
    "next.js",
    "react",
    "cli",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyi86/nextjs-devtools.git"
  },
  "license": "MIT",
  "author": "Henry Hong <henry.hong@kakao.com>",
  "type": "module",
  "bin": {
    "nextjs-devtools": "bin/index.js"
  },
  "scripts": {
    "build": "tsup",
    "postbuild": "chmod +x bin/*",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "dev": "tsx src/index.ts",
    "prepare": "pnpm build",
    "ci:publish": "pnpm publish"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.6.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-only-warn": "^1.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.1",
  "publishConfig": {
    "access": "public"
  }
}
